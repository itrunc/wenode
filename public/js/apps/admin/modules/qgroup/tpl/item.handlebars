<div class="card-content">
  <span class="card-title black-text">{{model.title}}</span>
</div>
<div class="card-action">{{model.detail}}</div>
<div class="card-action">
  {{#each model.tags}}
    <span class="wn-tag cyan darken-1 white-text z-depth-1">
        <i class="mdi-action-loyalty"></i>{{this}}
      </span>
  {{/each}}
</div>
<div class="card-action right-align">
  <a href="/admin#qchoice/{{model.objectId}}" class="teal-text">试题维护</a>
  <a href="#" class="btn-edit teal-text">编辑</a>
  <a href="#" class="btn-remove teal-text">删除</a>
</div>